                <translate x="1" y="0" z="1" />
                <scale x="0.8" y="0.8" z="0.8" />
            </transform>
            <models>
                <model file="../output/sphere_1_8_8.3d" />
            </models>
        </group>

        <group>
            <transform>
                <translate x="-1" y="0" z="1" />
                <scale x="0.5" y="0.5" z="0.5" />
            </transform>
            <models>
                <model file="../output/cone_1_2_4_3.3d" />
            </models>
        </group>
    </group>
</world>
